<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="%KeyValuesSender.keyValuesForm.selectOptions" />
      <RadioButton fx:id="keepExisting" mnemonicParsing="false" text="%KeyValuesSender.keyValuesForm.keepExisting" />
      <RadioButton fx:id="replaceExisting" mnemonicParsing="false" text="%KeyValuesSender.keyValuesForm.replaceExisting" />
      <RadioButton fx:id="deleteAll" mnemonicParsing="false" selected="true" text="%KeyValuesSender.keyValuesForm.deleteAll" />
   </children>
</fx:root>
